<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="590dp"
        android:layout_height="745dp"
        android:background="#00131111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/image" />


    <TextView
        android:id="@+id/welomeText"
        android:layout_width="353dp"
        android:layout_height="48dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="580dp"
        android:paddingLeft="70dp"
        android:textColor="#B1E0BD"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/latText"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="530dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="160dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welomeText"
    />

    <TextView
        android:id="@+id/lonText"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginBottom="470dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="160dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/latText"
    />

    <TextView
        android:id="@+id/accText"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="410dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="160dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lonText"
        />

    <TextView
        android:id="@+id/altText"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginBottom="350dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="160dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/accText"
        />
    <TextView
        android:id="@+id/adrText"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginBottom="290dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="160dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/altText"
        />


    <TextView
        android:id="@+id/adrVal"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginBottom="230dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="10dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/adrText"
        />

    <TextView
        android:id="@+id/latVal"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="80dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="530dp"
        android:inputType="number|numberDecimal"
        android:textColor="#808080"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welomeText" />

    <TextView

        android:id="@+id/lonVal"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="470dp"
        android:textColor="#808080"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/latText" />

    <TextView
        android:id="@+id/accVal"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="410dp"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="10dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lonText"
        />
    <TextView
        android:id="@+id/altVal"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginBottom="350dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="10dp"
        android:textColor="#808080"
        android:textStyle="italic"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/accText"
        />



</androidx.constraintlayout.widget.ConstraintLayout>